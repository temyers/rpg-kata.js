{
  "name": "rpg-kata",
  "version": "1.0.0",
  "description": "rpg-kata exercise in JavaScript",
  "main": "index.js",
  "watch": {
    "mocha": "{lib,test}/**/*{.js,.ts}",
    "cucumber": {
      "patterns": [
        "lib",
        "features"
      ],
      "extensions": "ts,js,feature"
    }
  },
  "scripts": {
    "test:unit": "nyc mocha",
    "test:accept": "nyc cucumber-js",
    "test": "npm-run-all --silent -c test:unit test:accept",
    "watch": "npm-watch"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:temyers/rpg-kata.js.git"
  },
  "keywords": [
    "kata",
    "bdd",
    "cucumber"
  ],
  "author": "Mechanical Rock",
  "homepage": "https://github.com/temyers/rpg-kata.js",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/cucumber": "^6.0.1",
    "@types/mocha": "^8.0.3",
    "chai": "^4.2.0",
    "cucumber": "^6.0.5",
    "mocha": "^8.1.3",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.7.0",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.19",
    "ts-mocha": "^7.0.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  }
}
