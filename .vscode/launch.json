  "version": "0.2.0",
  "configurations": [
  {
    "name": "NPM test:eventSourcing",
    "request": "launch",
    "runtimeArgs": [
      "run",
      "test:eventSourcing"
    ],
    "runtimeExecutable": "npm",
    "skipFiles": [
      "<node_internals>/**"
    ],
    "type": "node"
  },
  {
    "name": "NPM test:standard",
    "request": "launch",
    "runtimeArgs": [
      "run",
      "test:standard"
    ],
    "runtimeExecutable": "npm",
    "skipFiles": [
      "<node_internals>/**",
      "node_modules/**"
    ],
    "type": "node"
  },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Cucumber",
      "program": "${workspaceRoot}/node_modules/.bin/cucumber-js"
    }
  ]
}
